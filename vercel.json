{
  "buildCommand": "npm run build:prod",
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 0 * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-api-key",
          "value": "@api_secret_key"
        }
      ]
    }
  ]
}