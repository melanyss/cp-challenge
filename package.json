{
  "name": "cp-challenge",
  "version": "0.1.0",
  "description": "A simple app to track phone calls",
  "private": true,
  "author": "Melanys Figueredo <hello@melanys.me>",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"next dev\" \"open-cli http://localhost:3000\"",
    "generate-structure": "node --loader ts-node/esm generate-project-structure.ts",
    "build": "set NEXT_TELEMETRY_DISABLED=1 && npm run format && npm run add-paths && npm run generate-structure && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "add-paths": "python add_paths.py"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.46.1",
    "body-parser": "^1.20.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "lucide-react": "^0.461.0",
    "next": "15.0.3",
    "node-cron": "^3.0.3",
    "open-cli": "^8.0.0",
    "prettier": "^3.3.3",
    "react": "latest",
    "react-dom": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/node": "^20.17.8",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
